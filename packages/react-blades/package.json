{
  "name": "react-blades",
  "version": "0.0.1",
  "description": "Horizontal stacking window panels.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:js && npm run build:css && npm run build:components",
    "build:css": "node-sass src/css/styles.scss dist/css/_blades.css",
    "build:js": "babel src/js -d dist/ --ignore src/js/components",
    "build:components": "webpack",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.jsx src/js",
    "mocha": "mocha --compilers js:babel-register,jsx:babel-register --require airbnb-js-shims --recursive ./test/_helpers",
    "pretest": "npm run --silent lint",
    "tests-only": "npm run mocha --silent test",
    "tests-watch": "mocha --compilers js:babel-register,jsx:babel-register --require airbnb-js-shims --recursive ./test/_helpers --watch test",
    "tests-karma": "karma start",
    "test": "npm run tests-only && npm run tests-karma"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericberagg/react-blades.git"
  },
  "keywords": [
    "react",
    "react-blades",
    "blades",
    "blade"
  ],
  "author": "Eric Beragg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericberagg/react-blades/issues"
  },
  "homepage": "https://github.com/ericberagg/react-blades#readme",
  "devDependencies": {
    "airbnb-js-shims": "^1.0.1",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-airbnb": "^2.0.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "eslint": "^3.3.1",
    "enzyme": "^2.7.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "sinon-sandbox": "^1.0.2",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-dom": ">=0.14"
  }
}
