{
  "name": "react-blades",
  "version": "0.0.1",
  "description": "Horizontal stacking window panels.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:js && npm run build:css && npm run build:svg",
    "build:css": "node-sass css/styles.scss lib/css/_blades.css",
    "build:js": "babel src/ -d lib/ --ignore src/components",
    "build:svg": "webpack",
    "clean": "rimraf lib",
    "lint": "eslint --ext .js,.jsx src",
    "test": "babel-node src/__tests__/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericberagg/react-blades.git"
  },
  "keywords": [
    "react",
    "react-blades",
    "blades",
    "blade"
  ],
  "author": "Eric Beragg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericberagg/react-blades/issues"
  },
  "homepage": "https://github.com/ericberagg/react-blades#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-jsx": "^6.13.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-register": "^6.11.6",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "node-sass": "^3.8.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": "15.2.0",
    "react-dom": "15.2.0"
  }
}
