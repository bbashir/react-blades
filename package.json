{
  "name": "react-blades",
  "version": "0.0.10",
  "description": "Horizontal stacking window panels.",
  "main": "index.js",
  "scripts": {
    "build": "yarn run clean && yarn run build:js && yarn run build:css && yarn run build:components",
    "build:css": "node-sass src/css/styles.scss dist/css/Blades.css",
    "build:js": "babel src/js -d dist/ --ignore src/js/components",
    "build:components": "webpack",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.jsx src/js",
    "mocha": "mocha --compilers js:babel-register,jsx:babel-register --require airbnb-js-shims --recursive ./test/_helpers",
    "publish-patch": "npm version patch",
    "publish-minor": "npm version minor",
    "publish-major": "npm version major",
    "pretest": "npm run --silent lint",
    "tests-only": "karma start",
    "tests-watch": "karma start --auto-watch --no-single-run",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericontilt/react-blades.git"
  },
  "keywords": [
    "react",
    "react-blades",
    "blades",
    "blade"
  ],
  "author": "Eric Beragg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericontilt/react-blades/issues"
  },
  "homepage": "https://github.com/ericontilt/react-blades#readme",
  "devDependencies": {
    "airbnb-js-shims": "^1.0.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "6.4.1",
    "babel-preset-airbnb": "^2.0.0",
    "babel-register": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jasmine-core": "^2.6.1",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "node-sass": "^4.3.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-dom": ">=0.14"
  }
}
